{
  "name": "rockpaperscissors",
  "displayName": "Rock,Paper,Scissors",
  "author": {
    "name": "Sasen Perera"
  },
  "publisher": "SasenPerera",
  "repository": {
    "url": "https://github.com/Sas2k/Rock-Paper-Scissors-vs-code-extension"
  },
  "readme": "README.md",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Rock,Paper,Scissors.rock",
        "title": "Rock"
      },
      {
        "command": "Rock,Paper,Scissors.paper",
        "title": "Paper"
      },
      {
        "command": "Rock,Paper,Scissors.scissors",
        "title": "Scissors"
      }
    ],
    "configuration": {
      "title": "Rock,Paper,Scissors",
      "properties": {}
    }
  },
  "activationEvents": [
    "onCommand:Rock,Paper,Scissors.rock",
    "onCommand:Rock,Paper,Scissors.paper",
    "onCommand:Rock,Paper,Scissors.scissors"
  ]
}